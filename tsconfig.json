{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"lib": ["esnext", "DOM"],
		"moduleResolution": "node",
		"module": "esnext",
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": false,
		"target": "esnext",
		"paths": {
			"$src/*": ["src/*"],
			"$src": ["src"]
		}
	},
	"include": ["node_modules/sveltekit-zero-api/__temp.ts"]
}
